{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "opdsystem"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css"
      ],
      "scripts": [
        "./app/style/vendor/jquery/jquery.min.js",
        "./app/style/vendor/popper/popper.min.js",
        "./app/style/vendor/bootstrap/js/bootstrap.min.js",
        "./app/style/vendor/jquery-easing/jquery.easing.min.js",
        "./app/style/vendor/chart.js/Chart.min.js",
        "./app/style/vendor/datatables/jquery.dataTables.js",
        "./app/style/vendor/datatables/dataTables.bootstrap4.js",
        "./app/style/js/sb-admin.min.js",
        "./app/style/js/sb-admin-datatables.min.js",
        "./app/style/js/sb-admin-charts.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
